function validar()
{
var html = "";
if($('#cpf').validateCPF())
{
html += "<br/>O 1º CPF é válido.";
}
else
{
html += "<br/>O 1º CPF é <b style='color: #990000;'>inválido</b>.";
}
if($('#cpf2').validateCPF())
{
html += "<br/>O 2º CPF é válido.";
}
else
{
html += "<br/>O 2º CPF é <b style='color: #990000;'>inválido</b>.";
}
if($('#cpf3').validateCPF())
{
html += "<br/>O 3º CPF é válido.";
}
else
{
html += "<br/>O 3º CPF é <b style='color: #990000;'>inválido</b>.";
}
html = "<p>" + html + "</p>";
$('#r').html(html).position('center');

return false;
}
</script>